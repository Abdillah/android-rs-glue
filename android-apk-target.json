{
    "data-layout": "e-p:32:32:32-i1:8:8-i8:8:8-i16:16:16-i32:32:32-i64:64:64-f32:32:32-f64:64:64-v64:64:64-v128:64:128-a0:0:64-n32",
    "llvm-target": "arm-linux-androideabi",
    "target-endian": "little",
    "target-word-size": "32",
    "arch": "arm",
    "os": "android",
    "options": {
        "linker": "cc",
        "features": "+v7",
        "dynamic_linking": true,
        "executables": true,
        "morestack": true,
        "linker_is_gnu": true,
        "has_rpath": true,
        "pre_links_args": ["-Wl,--as-needed", "-Wl,--allow-multiple-definition", "-Wl,-E"],
        "position_independant_executables": true
    }
}
